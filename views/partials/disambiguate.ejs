<% if (appData.artists.length > 0 && appData.artistId === "") { %>
    <p>appData.artistId is <%= appData.artistId %></p>
    <section id="disambiguate">
      <h2>Disambiguate</h2>
      <form action="/disambiguate" method="post">
        <% appData.artists.forEach((artist) => { %>
        <div class="form-check">
          <input
            type="radio"
            class="form-check-input"
            name="artistId"
            id="<%= artist.id %>"
            value="<%= artist.id %>"
          />
          <label class="form-check-label" for="<%= artist.id %>">
            <ul>
              <li>Name: <%= artist.name %></li>
              <li>Score: <%= artist.score %></li>
              <li>Country: <%= artist.country %></li>
              <li>Note: <%= artist.disambiguation %></li>
            </ul>
          </label>
        </div>
        <% }) %>
        <input type="submit" class="btn btn-primary" value="Submit" />
      </form>
    </section>
    <% } %>