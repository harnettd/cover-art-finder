<% if (appData.artists.length > 0 && !appData.artistId) { %>
	<section id="disambiguate" class="mb-3">
		<p class="mb-1">Disambiguate</p>
			<form action="/disambiguate" method="post">
				<% appData.artists.forEach((artist) => { %>
					<div class="form-check">
							<input
								type="radio"
								class="form-check-input"
								name="artistId"
								id="<%= artist.id %>"
								value="<%= artist.id %>"
							/>
							<label class="form-check-label" for="<%= artist.id %>">
								<%= artist.name %>
								(Score: <strong><%= artist.score%></strong>)
								<%= artist.country %>,
								<em><%= artist.disambiguation %></em>
							</label>
					</div>
				<% }) %>
				<input type="submit" class="btn btn-primary rounded-2 mt-1" value="Submit" />
			</form>
	</section>
<% } %>